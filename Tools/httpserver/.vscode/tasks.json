{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "type": "typescript",
            "tsconfig": "tsconfig.json",
            "problemMatcher": [
                "$tsc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "upload",
            "type": "shell",
            "command": "./shell/upload.sh",
            "group": "build",
            "args": [
                "${workspaceFolder}",
            ],
            "problemMatcher": [
                "$go"
            ]
        }
    ]
}
